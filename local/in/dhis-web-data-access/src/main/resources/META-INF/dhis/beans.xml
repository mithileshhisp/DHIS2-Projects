<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xsi:schemaLocation="
http://www.springframework.org/schema/beans http://www.springframework.org/schema/beans/spring-beans-3.1.xsd">

	<!-- Common Action  -->
	
	<bean id="org.hisp.dhis.dataaccess.action.HomePageAction"
        class="org.hisp.dhis.dataaccess.action.HomePageAction"
        scope="prototype">
    </bean>
    
	<bean id="org.hisp.dhis.dataaccess.action.ExportToExcelAction"
        class="org.hisp.dhis.dataaccess.action.ExportToExcelAction"
        scope="prototype">		
    </bean>


	<!--Data Inventory Status Action-->

    <bean id="org.hisp.dhis.dataaccess.action.DataInventoryStatusAction"
          class="org.hisp.dhis.dataaccess.action.DataInventoryStatusAction"
          scope="prototype">
        <property name="dataSetService" ref="org.hisp.dhis.dataset.DataSetService" />
        <property name="jdbcTemplate" ref="jdbcTemplate"/>
    </bean>                                                                                 


    <bean id="org.hisp.dhis.dataaccess.action.DataInventoryStatusFormAction"
          class="org.hisp.dhis.dataaccess.action.DataInventoryStatusFormAction"
          scope="prototype">
        <property name="dataSetService" ref="org.hisp.dhis.dataset.DataSetService" />
        <property name="dataElementService" ref="org.hisp.dhis.dataelement.DataElementService"/>
    </bean>


    <!--Data Search Action-->

    <bean id="org.hisp.dhis.dataaccess.action.SearchByDataelements"
          class="org.hisp.dhis.dataaccess.action.SearchByDataelements"
          scope="prototype">
        <property name="dataElementService" ref="org.hisp.dhis.dataelement.DataElementService" />
    </bean>

    <bean id="org.hisp.dhis.dataaccess.action.SearchStartEndDateAction"
          class="org.hisp.dhis.dataaccess.action.SearchStartEndDateAction"
          scope="prototype">
        <property name="dataElementService" ref="org.hisp.dhis.dataelement.DataElementService" />
        <property name="jdbcTemplate" ref="jdbcTemplate"/>
        <property name="dataValueService" ref="org.hisp.dhis.datavalue.DataValueService"/>
    </bean>


    <bean id="org.hisp.dhis.dataaccess.action.GetSearchDataResultAction"
          class="org.hisp.dhis.dataaccess.action.GetSearchDataResultAction"
          scope="prototype">
        <property name="dataElementService" ref="org.hisp.dhis.dataelement.DataElementService" />
        <property name="jdbcTemplate" ref="jdbcTemplate"/>
    </bean>

    <bean id="org.hisp.dhis.dataaccess.action.GetDataValuesFormAction"
          class="org.hisp.dhis.dataaccess.action.GetDataValuesFormAction"
          scope="prototype">
        <property name="dataElementService" ref="org.hisp.dhis.dataelement.DataElementService" />
    </bean>

    <bean id="org.hisp.dhis.dataaccess.action.GetDataValuesAction"
          class="org.hisp.dhis.dataaccess.action.GetDataValuesAction"
          scope="prototype">
        <property name="dataElementService" ref="org.hisp.dhis.dataelement.DataElementService" />
        <property name="jdbcTemplate" ref="jdbcTemplate"/>
    </bean>


	
</beans>