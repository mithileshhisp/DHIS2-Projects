<!DOCTYPE hibernate-mapping PUBLIC
  "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
  "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd"
  >

<hibernate-mapping>
  <class name="org.hisp.dhis.schedule.SchedulingPolicy" table="schedulingpolicy">
    <id name="id" column="schedulingpolicyid">
      <generator class="native"/>
    </id>

    <property name="name" column="name" not-null="true" unique="true" length="255"/>
    <property name="value" column="value" not-null="true" unique="true" length="255"/>
           
  </class>
</hibernate-mapping>
