<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xsi:schemaLocation="
http://www.springframework.org/schema/beans http://www.springframework.org/schema/beans/spring-beans-3.1.xsd">

	<bean id="org.hisp.dhis.dataverify.DataVerificationStore" class="org.hisp.dhis.dataverify.hibernate.HibernateDataVerificationStore">
		<property name="clazz" value="org.hisp.dhis.dataverify.DataVerification" />
		<property name="sessionFactory" ref="sessionFactory" />
	</bean>

		<bean id="org.hisp.dhis.dataverify.DataVerificationService" class="org.hisp.dhis.dataverify.DefaultDataVerificationService">
		<property name="dataVerificationStore" ref="org.hisp.dhis.dataverify.DataVerificationStore" />
	</bean>

	</beans>
