            <option value=$dataelement.getId()>$dataelement.getName()</option>
        #end
    </select>

    <br>
    <br>
    <br>

    <script>
        $(".chzn-select").chosen();
    </script>

    <input name="Get Data" type="submit" value="Get Data">

</form>