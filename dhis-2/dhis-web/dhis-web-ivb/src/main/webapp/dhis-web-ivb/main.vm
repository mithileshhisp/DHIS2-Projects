 <script>
 	$(document).ready(function() {
	 	$( ":button" ).button();
		$( ":submit" ).button();
		
		jQuery.ajax({
         url: "getAuthorities.action", 
         dataType: 'json',      
         success: function(json){            
			var auth = json;
			if(jQuery.trim(auth.conflict) == "Yes")
			{				
				document.getElementById("userConflict").style.display = 'block';				
			}
			if(jQuery.trim(auth.activity) == "Yes")
			{
				document.getElementById("userActivity").style.display = 'block';
			}
			if(jQuery.trim(auth.userActivityLogin) == "Yes")
			{
				document.getElementById("userActivityLogin").style.display = 'block';
			}
			if(jQuery.trim(auth.userActivityLoginMonitoringAuthority) == "Yes")
			{
				document.getElementById("userActivityLoginMonitoring").style.display = 'block';
			}
			if(jQuery.trim(auth.dashboard) == "Yes")
			{
				document.getElementById("userDashboard").style.display = 'block';
			}
			if(jQuery.trim(auth.aggEngine) == "Yes")
			{				
				document.getElementById("aggregationQueryBuilder").style.visibility = 'visible';
			}
			if(jQuery.trim(auth.lookup) == "Yes")
			{				
				document.getElementById("lookup").style.visibility = 'visible';
			}
			if(jQuery.trim(auth.maintenanceModules) == "Yes")
			{				
				document.getElementById("menu_maintenance").style.visibility = 'visible';
			}
			if(jQuery.trim(auth.progReviewTAReportAuthority) == "Yes")
			{							
				document.getElementById("progReviewTAReport").style.display = 'block';
			}
			if(jQuery.trim(auth.cmypDevTrackingReportAuthority) == "Yes")
			{							
				document.getElementById("cmypDevTrackingReport").style.display = 'block';
			}
			if(jQuery.trim(auth.alertReportAuthority) == "Yes")
			{				
				document.getElementById("alertReport").style.display = 'block';				
			}
         }
      });
	
	});
 	$(function() {
 		var subject = $( "#subject" ),
			text = $( "#text" ),			
			allFields = $( [] ).add( subject ).add( text ),
			tips = $( ".validateTips" );
			
 		function checkLength( o, n, min) {
			if ( o.val().length < min ) {
				o.addClass( "ui-state-error" );
				updateTips( "Plese Enter "+ n );
				return false;
			} else {
				return true;
			}
		}
		function updateTips( t ) {
			tips
				.text( t )
				.addClass( "ui-state-highlight" );
			setTimeout(function() {
				tips.removeClass( "ui-state-highlight", 1500 );
			}, 500 );
		}
	 	$( "#showSendFeedback" ).dialog({
				autoOpen: false,
				height: 380,
				width: 500,
				modal: true,
				show: "blind",
				hide: "explode",
				buttons: {
				"$i18n.getString( 'send' )":function() {
				var bValid = true;
				allFields.removeClass( "ui-state-error" );
				
				bValid = bValid && checkLength( subject, "Subject", 1) && checkLength( $('textarea#text'), "Text", 1);	
				
				if ( bValid == true ) {
						var dataValue = {
				        'subject' : subject.val(),
				        'text' : $('textarea#text').val()
    				};
			   		 jQuery.ajax( {
			            url: 'sendFeedback.action',
			            data: dataValue,
			            dataType: 'json',
			            success: handleSuccess,
			            error: handleError
			        } );
						$( this ).dialog( "close" );
					}
				},
				Cancel: function() {
					$( this ).dialog( "close" );
				}
			},
			close: function() {
				allFields.val( "" ).removeClass( "ui-state-error" );
			}
		});
		
		$( "#profileInfo" ).dialog({
				autoOpen: false,				
				width: 421,
				modal: true,
				show: "blind",
				hide: "explode"
			
		});
		
		$( "#profile" ).click(function() {		
		$('#profileInfo').load("showUpdateUserAccountForm.action", 
			{				
			}
			, function( ){
				$( "#profileInfo" ).dialog( "open" );				
		});	
		
			
			return false;
		});
		$( "#contactus" ).click(function() {
			$( "#showSendFeedback" ).dialog( "open" );
			return false;
		});
	function handleSuccess( json )
    {
       showSuccessMessage( "Message Send" );
    }
    function handleError( jqXHR, textStatus, errorThrown )
    {       
       showWarningMessage( "Message Not Send" );
    }
	
	});
 	function changeLocale( locale )
 	{
 			
		var dataValue = {        
	        'currentLocale' : locale
	    };
	    jQuery.ajax( {
	            url : 'changeLocale.action',
	            data : dataValue,
	            dataType : 'json',	            
	            success : currentLocation
	        } );
	    function currentLocation( json )
	    {
	    	window.location.href = window.location.href;
	    	
	    }
 	}
 	function hideMenu()
	{
		document.getElementById("leftMenu").style.display="none";
		document.getElementById("mainMenu").style.width = "1%";
		document.getElementById("hideMenu").style.display="none";
		document.getElementById("showMenu").style.display="block";
		document.getElementById("mainPage").style.width = "97%";
	}
	function showMenu()
	{
		document.getElementById("leftMenu").style.display="block";
		document.getElementById("mainMenu").style.width = "18%";
		document.getElementById("hideMenu").style.display = "block";
		document.getElementById("showMenu").style.display = "none";
		document.getElementById("mainPage").style.width = "80%";
	}

	
