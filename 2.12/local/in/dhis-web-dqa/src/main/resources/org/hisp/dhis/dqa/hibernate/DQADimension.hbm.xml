<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC
  "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
  "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd"
  [<!ENTITY identifiableProperties SYSTEM "classpath://org/hisp/dhis/common/identifiableProperties.hbm">]
  >


<hibernate-mapping>
  <class name="org.hisp.dhis.dqa.api.DQADimension" table="dqadimension">

    <cache usage="read-write" />

    <id name="id" column="dqadimensionid">
      <generator class="native" />
    </id>
    &identifiableProperties;
	
	<property name="name" column="name" not-null="true" unique="true" length="230" /> 
	<property name="description" type="text" /> 
	<property name="type" column="type" length="230" />   
	
	  
    <set name="indicators" table="dqadimension_indicators">
      <key column="dqadimensionid" foreign-key="fk_dqadimension_indicators_dqadimensionid" />
      <many-to-many class="org.hisp.dhis.indicator.Indicator" column="indicatorid"
        foreign-key="fk_dqadimension_indicatorid" />
    </set>	  
	  
	<!--
    <set name="validationRules" table="dqadimension_validationrules">
      <key column="dqadimensionid" foreign-key="fk_dqadimension_validationrules_dqadimensionid" />
      <many-to-many class="org.hisp.dhis.validation.ValidationRule" column="validationruleid"
        foreign-key="fk_dqadimension_validationruleid" />
    </set>	  
	-->  	  
	  
	     
  </class>
</hibernate-mapping>
