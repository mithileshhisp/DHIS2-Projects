					<td style="background-color:$curColor">NOT VISITED</font></td>
				
				#elseif( $curColor == "green" )
					<td style="background-color:$curColor"><a href="javascript:gotoMonthlyVisitViewForm( '$!patient.id', '$period.getExternalId()' )">VISITED</font></a></td>
				#else
                    <td style="background-color:$curColor">&nbsp;</td>					
				#end		
		   #end	
