		<th>Jul to Sep</th>
		<th>Oct to Dec</th>
		
	</tr>
	
	#foreach( $year in $years )
		<tr align="center">
			<td style="background-color:#EFFBEF;">$year</td>
			
			#set( $periods = $quarterlyPeriodTypeMap.get( $year ) )
			
            #foreach( $period in $periods )
				#set( $curColor = "grey" )
				
				#set( $curColor = $visitDateMap.get( $period.getExternalId() ) )
				
				#if( $curColor == "red" )
