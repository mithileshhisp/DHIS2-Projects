<input type="hidden" id='selectedOrgunitID' value="$!organisationUnit.id" />
<div id ="ashaManagementDiv" ></div><!-- ASHA Management Div -->
<div id='addUpdateASHAProfileDiv'></div> <!-- Add / Edit ASHA Profile Form -->
<div id='resultSearchDiv' style='font-size:13px'></div> <!-- List searching ASHAS -->

<div id ="ashaMasterChartReportDiv" >
<form id="asha_master_chart_report_form" name="asha_master_chart_report_form" action="ashaMasterChartReport.action" method="post" target="_blank" >
	<table>
		<tr>
			<th colspan="2">$i18n.getString( "asha_master_chart_report_form" ) - $!organisationUnit.name</th>
		</tr>
		<tr>
			<td>$i18n.getString( "start_month" )</td>
			<td>
				<select id="startMonth" name="startMonth" >
					<option value="-1">[$i18n.getString( "select" )]</option>
					#foreach( $period in $periods )
						<option value="$period.getExternalId()">$encoder.htmlEncode( $period.name )</option>
					#end				
				</select>
			</td>
		</tr>
		<tr>
			<td>&nbsp;</td>
			<td>&nbsp;</td>
		</tr>		
		<tr>
			<td>$i18n.getString( "end_month" )</td>
			<td>
				<select id="endMonth" name="endMonth" >
					<option value="-1">[$i18n.getString( "select" )]</option>
					#foreach( $period in $periods )
						<option value="$period.getExternalId()">$encoder.htmlEncode( $period.name )</option>
					#end
				</select>
			</td>
		</tr>
		<tr>
			<td>&nbsp;</td>
			<td>&nbsp;</td>
		</tr>
		<tr>
			<td>&nbsp;</td>
			<td>
				<input type="submit" value='$i18n.getString("generate_report")' />
				<input type="hidden" id="orgUnitId" name="orgUnitId" value="$!organisationUnit.id" />
			</td>
		</tr>
	</table>
</form>
</div>	