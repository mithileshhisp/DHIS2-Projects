<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xsi:schemaLocation="
http://www.springframework.org/schema/beans http://www.springframework.org/schema/beans/spring-beans-3.1.xsd">

	<!-- Store -->
	<bean id="org.hisp.dhis.beneficiary.BeneficiaryStore"
		class="org.hisp.dhis.beneficiary.hibernate.HiberateBeneficiaryStore">
		<property name="sessionFactory" ref="sessionFactory"/>
		<property name="periodStore" ref="org.hisp.dhis.period.PeriodStore"/>
	</bean>

	<!-- Service -->
	<bean id="org.hisp.dhis.beneficiary.BeneficiaryService"
		class="org.hisp.dhis.beneficiary.DefaultBeneficiaryService">
		<property name="beneficiaryStore" ref="org.hisp.dhis.beneficiary.BeneficiaryStore"/>
	</bean>
	
	<!-- Deletion Handlers -->
	
</beans>
