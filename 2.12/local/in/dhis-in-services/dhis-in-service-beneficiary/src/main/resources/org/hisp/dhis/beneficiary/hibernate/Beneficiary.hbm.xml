<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC
 "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
 "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">

<hibernate-mapping>
  <class name="org.hisp.dhis.beneficiary.Beneficiary" table="beneficiary">

	<id name="id" column="beneficiaryid">
      <generator class="native" />
    </id>
    
    <property name="name" not-null="true" length="160" />
    
    <property name="fatherName" column="fathername" not-null="false" length="160" />
	  
	<property name="gender" column="gender" not-null="true" length="5"  />    
    
    <property name="village" column="village" not-null="false" length="250" />
	  
	<property name="identifier" column="identifier" not-null="false" length="160" />
	
    
	<many-to-one name="period" class="org.hisp.dhis.period.Period" column="periodid"
      not-null="true" foreign-key="fk_beneficiary_periodid" />

	  
	<many-to-one name="dataElementGroup" class="org.hisp.dhis.dataelement.DataElementGroup" column="dataelementgroupid"
      not-null="true" foreign-key="fk_beneficiary_dataelementgroupid" />
	  
	  
	<many-to-one name="dataElement" class="org.hisp.dhis.dataelement.DataElement" column="dataelementid"
      not-null="true" foreign-key="fk_beneficiary_dataelementid" />
	  
	  
	<property name="price" column="price" not-null="false" length="50" />
	  
	<property name="registrationDate" column="registrationdate" not-null="true" />
	
	<property name="serviceGivenDate" column="servicegivendate" not-null="true" />
	  
	<many-to-one name="patient" class="org.hisp.dhis.patient.Patient" column="patientid"
      not-null="true" foreign-key="fk_beneficiary_patientid" />	  	  	  
	  	  
  </class>
</hibernate-mapping>