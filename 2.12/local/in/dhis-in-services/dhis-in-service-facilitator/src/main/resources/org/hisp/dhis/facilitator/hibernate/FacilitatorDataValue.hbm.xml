<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC
  "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
  "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">

<hibernate-mapping>
  <class name="org.hisp.dhis.facilitator.FacilitatorDataValue" table="facilitatordatavalue">

    <composite-id>
      <key-many-to-one name="facilitator" class="org.hisp.dhis.facilitator.Facilitator" column="facilitatorid" foreign-key="fk_facilitatordatavalue_facilitatorid" />
	  <key-many-to-one name="patient" class="org.hisp.dhis.patient.Patient" column="patientid" foreign-key="fk_facilitatordatavalue_patientid" />
	  <key-many-to-one name="period" class="org.hisp.dhis.period.Period" column="periodid" foreign-key="fk_datavalue_periodid" />  	
	  <key-many-to-one name="dataElement" class="org.hisp.dhis.dataelement.DataElement" column="dataelementid" foreign-key="fk_datavalue_dataelementid" />          
      <key-many-to-one name="optionCombo" class="org.hisp.dhis.dataelement.DataElementCategoryOptionCombo" column="categoryoptioncomboid" foreign-key="fk_datavalue_categoryoptioncomboid" />
    </composite-id>

    <property name="value" />

    <property name="storedBy" column="storedby" length="100" />

    <property name="lastUpdated" column="lastupdated" type="date" />

    <property name="comment" length="360" />

  </class>
</hibernate-mapping>
