<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xsi:schemaLocation="
http://www.springframework.org/schema/beans http://www.springframework.org/schema/beans/spring-beans-3.1.xsd">

	<!-- Store -->
	<bean id="org.hisp.dhis.tariffdatavalue.TariffDataValueStore"
		class="org.hisp.dhis.tariffdatavalue.hibernate.HibernateTariffDataValueStore">
		<property name="sessionFactory" ref="sessionFactory"/>
	</bean>

	<!-- Service -->
	<bean id="org.hisp.dhis.tariffdatavalue.TariffDataValueService"
		class="org.hisp.dhis.tariffdatavalue.DefaultTariffDataValueService">
		<property name="tariffDataValueStore" ref="org.hisp.dhis.tariffdatavalue.TariffDataValueStore"/>
	</bean>
	
	<!-- Deletion Handlers -->
	
</beans>
