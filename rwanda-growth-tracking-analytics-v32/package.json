{
  "name": "fbfanalytics",
  "version": "1.0",
  "description": "Growth tracking analytics for DHIS",
  "main": "src/index.html",
  "scripts": {
    "coverage": "babel-node node_modules/.bin/isparta cover --root ./src --report text --report html --report lcov node_modules/mocha/bin/_mocha -- --reporter dot --require config/setup.js --recursive test",
    "prebuild": "(rm -r build && mkdir build) || mkdir build",
    "build": "webpack",
    "postbuild": "cp index.html build/ && cp icon.png build/ && cp node_modules/jquery/dist/jquery.min.js build/ && cp node_modules/babel-polyfill/dist/polyfill.min.js build/ && cp manifest.webapp build/ && cd build && zip -r FBF_Analytics_60_months.zip *",
    "test": "mocha --reporter spec --compilers js:babel-core/register --require config/setup.js --recursive test",
    "test-watch": "mocha --reporter spec --compilers js:babel-core/register --require config/setup.js --recursive test --watch",
    "lint": "eslint --fix src && ./node_modules/eslint/bin/eslint.js --env node,mocha --global expect,sinon,spy,stub --rule 'no-unused-expressions: 0' test && scss-lint",
    "validate": "npm ls",
    "start": "webpack-dev-server"
  },
  "keywords": [
    "DHIS2",
    "App"
  ],
  "author": "J",
  "license": "BSD-3-Clause",
  "dependencies": {
    "babel-eslint": "^8.2.1",
    "babel-polyfill": "^6.26.0",
    "css-loader": "^0.28.9",
    "d2": "^28.3.0",
    "d2-ui": "^28.1.1",
    "eslint-config-airbnb": "^16.1.0",
    "eslint-config-prettier": "^2.9.0",
    "eslint-plugin-import": "^2.8.0",
    "eslint-plugin-jsx-a11y": "^6.0.3",
    "highcharts-exporting": "^0.1.7",
    "highcharts-offline-exporting": "^0.1.7",
    "jquery": "^2.2.3",
    "prop-types": "^15.6.0",
    "react": "^16.2.0",
    "react-dom": "^16.2.0",
    "react-flatpickr": "^3.6.3",
    "react-highcharts": "^16.0.1",
    "react-tap-event-plugin": "^3.0.2"
  },
  "devDependencies": {
    "babel-core": "^6.26.0",
    "babel-loader": "^7.1.2",
    "babel-preset-es2015": "^6.24.1",
    "babel-preset-react": "^6.24.1",
    "babel-preset-stage-0": "^6.24.1",
    "d2-manifest": "^1.0.0",
    "d2-utilizr": "^0.2.15",
    "eslint": "^4.16.0",
    "eslint-plugin-react": "^7.6.0",
    "material-ui": "^0.20.0",
    "react-stub-context": "^0.8.1",
    "rxjs": "^5.5.6",
    "style-loader": "^0.20.1",
    "webpack": "^3.10.0",
    "webpack-dev-server": "^2.11.1"
  },
  "pre-commit": [
    "test",
    "lint",
    "validate"
  ],
  "directories": {
    "test": "test"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/jonasbhe/growth-tracking-analytics.git"
  },
  "bugs": {
    "url": "https://github.com/jonasbhe/growth-tracking-analytics/issues"
  },
  "homepage": "https://github.com/jonasbhe/growth-tracking-analytics#readme",
  "manifest.webapp": {
    "icons": {
      "48": "icon.png"
    }
  }
}
